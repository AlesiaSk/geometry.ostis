sc_agent_of_finding_definitional_domain
=> nrel_main_idtf:
	[sc-агент поиска области определения для заданного отношения] (* <- lang_ru;; *);
	[sc-agent of finding definitional_domain for given relation] (* <- lang_en;; *);

<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);

=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_finding_definitional_domain_condition => ..sc_agent_of_finding_definitional_domain_result);

<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	question_of_finding_definitional_domain
	};

<- rrel_key_sc_element:
	...
	(*
	<- sc_description_of_sc_agent_behavior;;
	<= nrel_sc_text_translation:
		...
		(*
		-> rrel_example:
			"file://Agent_Definitional_domain.html";;
		*);;
	*);

<- rrel_key_sc_element:
	...
	(*
	<- sc_illustration;;
	<= nrel_sc_text_translation:
		...
		(*
		-> rrel_example:
			"file://domain.png";;
		*);;
	*);

=> nrel_inclusion: 
	...
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_finding_definitional_domain;
		proc_finding_combination	
		};;
	-> sc_agent_of_finding_definitional_domain_scp    
		(* 
		<- active_sc_agent;; 
		*);; 
	*);;

..sc_agent_of_finding_definitional_domain_condition
= [*
	question_of_finding_definitional_domain _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter;;
*];;

..sc_agent_of_finding_definitional_domain_result
= [*
	question_of_finding_definitional_domain _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._parameter;;
*];;
