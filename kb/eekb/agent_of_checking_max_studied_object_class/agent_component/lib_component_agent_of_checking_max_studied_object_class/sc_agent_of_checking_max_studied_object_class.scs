sc_agent_of_checking_max_studied_object_class
=> nrel_main_idtf:	
	[sc-агент проверки наличия максимального класса объектов исследования у предметных областей] (* <- lang_ru;; *);
	[sc-agent of сhecking maximum studied object class] (* <- lang_en;; *);
	
<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);
	
=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_checking_max_studied_object_class_condition => ..sc_agent_of_checking_max_studied_object_class_result);
	
<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	action_of_finding_max_class
	};
	
=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_checking_max_studied_object_class
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_checking_max_studied_object_class
		//proc_of_adding_validating_message
		};;
		
	-> sc_agent_of_checking_max_studied_object_class_scp 
		(* <- active_sc_agent;; *);;
	*);;

..sc_agent_of_checking_max_studied_object_class_condition
= [*
	action_of_finding_max_class _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter1;;
*];;

..agent_of_checking_max_studied_object_class_result
= [*
	action_of_finding_max_class _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer_without_maxclass;;
	.._question _-> .._parameter1;;
	
*];;
